// Prisma schema for registration form with Neon DB
// Add your Neon DB connection string in .env as DATABASE_URL

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Registration {
  id                Int      @id @default(autoincrement())
  cnicOrStudentCard String
  cnicOrStudentCardUrl String
  transactionId     String
  accountNo         String
  paymentSlipUrl    String
  createdAt         DateTime @default(now())
}
